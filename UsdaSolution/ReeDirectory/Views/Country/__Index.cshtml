@model IEnumerable<ReeDirectoryEntityFm.Entities.General.ECountry>

<table class="table">
    <tr>
        <th>
            <a href="#" onclick="return SetSort(this)">
                @Html.DisplayNameFor(model => model.Abbrivation)
            </a>
        </th>

        <th>
            <a href="#" onclick="return SetSort(this)">
                @Html.DisplayNameFor(model => model.Name)
            </a>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Abbrivation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
               @Html.Partial("__ActionControl", item)
            </td>
        </tr>
    }
</table>
