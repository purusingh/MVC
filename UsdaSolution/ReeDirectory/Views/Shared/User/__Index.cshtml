@model IEnumerable<ReeDirectoryEntityFm.Entities.Security.EUser>

<table class="table table-striped">
    <tr>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.LoginName)
            </a>
        </th>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.FirstName)
            </a>
        </th>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.MidName)
            </a>
        </th>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.LastName)
            </a>
        </th>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.Email)
            </a>
        </th>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.Phone)
            </a>
        </th>
        <th>
            <input type="submit" onclick="return print();" />
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LoginName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MidName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.Partial("__ActionControl", item)
            </td>
        </tr>
    }

</table>
