@model IEnumerable<ReeDirectory.EntityFM.Entities.Security.EController>
<table class="table">
    <tr>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.Name)
            </a>
        </th>
        <th>
            <a href="#" onclick="SetSort(this)">
                @Html.DisplayNameFor(model => model.Path)
            </a>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Path)
            </td>
            <td>
                @Html.Partial("__ActionControl", item)
            </td>
        </tr>
    }

</table>
