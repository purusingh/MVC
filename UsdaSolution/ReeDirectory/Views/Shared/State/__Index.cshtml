@model IEnumerable<ReeDirectory.EntityFM.Entities.EState>

@{
    ReeDirectory.EntityFM.ExternalEntity.ESecurity sec = ViewBag.Security as ReeDirectory.EntityFM.ExternalEntity.ESecurity;
 
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Abbrivation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Country)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Abbrivation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Country.Name)
        </td>
        <td>
           @Html.Partial("__ActionControl", item)
        </td>
    </tr>
}

</table>
